{
	"name": "Swipe Right for Facebook",
	"version": "1.0.0",
	"manifest_version": 2,
	"web_accessible_resources": ["app.js"],
	"content_scripts": [
		{
			"matches": [
				"http://*.facebook.com/*",
				"http://facebook.com/*",
				"https://*.facebook.com/*",
				"https://facebook.com/*"
			],
			"js": [
				"app.js"
			]
		}
	]
}